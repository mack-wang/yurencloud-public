{
  "entry": "src/index.js",
  "env": {
    "development": {
      "extraBabelPlugins": [
        "dva-hmr",
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }],
        ["module-resolver", {
              "root": ["./src"],
              "alias": {
                "components": "./src/components",
                "utils": "./src/utils",
                "config": "./src/utils/config",
                "services": "./src/services",
                "models": "./src/models",
                "routes": "./src/routes",
                "assets": "./src/assets"
              }
            }]
      ]
    },
    "production": {
      "extraBabelPlugins": [
        "transform-runtime",
        ["import", { "libraryName": "antd", "style": "css" }],
        ["module-resolver", {
                      "root": ["./src"],
                      "alias": {
                        "components": "./src/components",
                        "utils": "./src/utils",
                        "config": "./src/utils/config",
                        "services": "./src/services",
                        "models": "./src/models",
                        "routes": "./src/routes",
                        "assets": "./src/assets"
                      }
                    }]
      ]
    }
  },
  "proxy": {
    "/api": {
      "target": "http://www.yurencloud.com:8080/",
      "changeOrigin": true,
      "pathRewrite": { "^/api" : "" }
     }
  }
}
